generator client {
  provider = "prisma-client-js"
  output   = "./generated/client_main"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_ACESSOS")
}

model t_acessos {
  id        Int      @id(map: "PK_t_acessos") @default(autoincrement())
  login     String?  @db.VarChar(60)
  senha     String?  @db.VarChar(15)
  nome      String?  @db.VarChar(30)
  funcao    String?  @db.VarChar(30)
  banco     String?  @db.VarChar(100)
  adm       String?  @db.Char(1)
  ativo     String?  @db.Char(1)
  con       Int?
  pwd       String?  @db.Char(1)
  cnpj      String?  @db.VarChar(18)
  ddd       String?  @db.VarChar(2)
  whatsapp  String?  @db.VarChar(10)
  createdAt DateTime @default(now(), map: "DF_t_acessos_createdAt") @db.DateTime
  Empresa   String?  @db.VarChar(100)
  logoUrl   String?  @db.VarChar(Max)

  @@map("t_acessos")
}

model t_banco {
  id     Int       @id(map: "PK_T_BANCOS") @default(autoincrement())
  banco  String?   @db.VarChar(50)
  hscode String?   @db.VarChar(30)
  data   DateTime? @db.DateTime
}

model t_log {
  reglog  Int       @id(map: "PK_t_log") @default(autoincrement())
  data    DateTime? @db.DateTime
  login   String    @db.VarChar(60)
  usuario String?   @db.VarChar(30)
  empresa String?   @db.VarChar(40)
  loja    String?   @db.VarChar(20)
  motivo  String?   @db.VarChar(120)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model versao {
  aplicativo String?   @db.VarChar(50)
  compilacao DateTime? @db.DateTime

  @@ignore
}
